{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "builds": [{ "src": "startStreamConsumer.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "startStreamConsumer.js" }],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "startStreamConsumer.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "regions": ["iad1"],
  "public": false
}
